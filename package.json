{
  "publisher": "bruceyuhb",
  "name": "hsl-preview",
  "displayName": "HSL Color Preview",
  "version": "0.0.2",
  "description": "Preview HSL color in vscode, for CSS, SCSS, LESS.",
  "author": "Bruce Yu <bruceyuhongbo@gmail.com>",
  "homepage": "https://github.com/13ruceYu/hsl-preview#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/13ruceYu/hsl-preview"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "pnpm vsce package --no-dependencies",
    "publish": "pnpm vsce publish --no-dependencies",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "bumpp": "^9.6.1",
    "eslint": "^9.9.0",
    "typescript": "^5.4.5"
  }
}
